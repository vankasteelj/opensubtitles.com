{
  "/infos/formats": {
    "url": "/api/v1/infos/formats",
    "method": "GET"
  },
  "/infos/user": {
    "opts": {
      "auth": true
    },
    "url": "/api/v1/infos/user",
    "method": "GET"
  },
  "infos/languages": {
    "url": "/api/v1/infos/languages",
    "method": "GET"
  },
  "/login": {
    "url": "/api/v1/login",
    "method": "POST",
    "body": {
      "username": null,
      "password": null
    },
    "/logout": {
      "url": "/api/v1/logout",
      "method": "DELETE"
    },
    "/discover/popular": {
      "url": "/api/v1/discover/popular?languages=&type=",
      "optional": ["languages", "type"],
      "method": "GET"
    },
    "/discover/latest": {
      "url": "/api/v1/discover/latest?languages=&type=",
      "optional": ["languages", "type"],
      "method": "GET"
    },
    "/discover/most_downloaded": {
      "url": "/api/v1/discover/most_downloaded?languages=&type=",
      "optional": ["languages", "type"],
      "method": "GET"
    },
    "/download": {
      "opts": {
        "auth": true
      },
      "url": "/api/v1/download",
      "method": "POST",
      "body": {
        "file_id": null,
        "sub_format": null,
        "file_name": null,
        "in_fps": null,
        "out_fps": null,
        "timeshift": null,
        "force_download": null
      }
    }, 
    "/subtitles": {
      "url": "/api/v1/subtitles?ai_translated=&episode_number=&foreign_parts_only=&hearing_impaired=&id=&imdb_id=&languages=&machine_translated=&moviehash=&moviehash_match=&order_by=&order_direction=&page=&parent_feature_id=&parent_imdb_id=&parent_tmdb_id=&query=&season_number=&tmdb_id=&trusted_sources=&type=&user_id=&year=",
      "optional": ["ai_translated", "episode_number", "foreign_parts_only", "hearing_impaired", "id", "imdb_id", "languages", "machine_translated", "moviehash", "moviehash_match", "order_by", "order_direction", "page", "parent_feature_id", "parent_imdb_id", "parent_tmdb_id", "query", "season_number", "tmdb_id", "trusted_sources", "type", "user_id", "year"],
      "method": "GET"
    }, 
    "/features": {
      "url": "/api/v1/features?query=&feature_id=&imdb_id=&tmdb_id=&type=&year=",
      "optional": ["feature_id", "imdb_id", "tmdb_id", "type", "year"],
      "method": "GET"
    },
    "/guessit": {
      "url": "/api/v1/guessit?filename=", 
      "optional": ["filename"],
      "method": "GET"
    }
  }
}